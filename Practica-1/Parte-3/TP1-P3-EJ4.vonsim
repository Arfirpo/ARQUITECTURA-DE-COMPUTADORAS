;Ejercicio 4 - Multiplicaci√≥n de numero sin signo c/ parametros

;4A)
                ORG 1000H
A               DB 5
B               DB 0
RES             DW ?

                ORG 3000H
MULTI:          PUSH CX
                PUSH DX
                CMP AL, 0
                JZ VOLVER
                CMP AH, 0
                JZ VOLVER
                MOV CH, 0
                MOV CL, AL
                MOV DX, 0
LOOP:           ADD DX, CX
                DEC AH
                JNZ LOOP
VOLVER:         MOV AX, DX
                POP DX
                POP CX
                RET

                ORG 2000H
                MOV AL, A
                MOV AH, B
                CALL MULTI
                MOV RES, AX
                INT 0
END
