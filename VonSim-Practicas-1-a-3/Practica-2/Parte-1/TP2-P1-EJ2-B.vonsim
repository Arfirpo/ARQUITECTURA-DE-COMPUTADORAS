PA		EQU 30H
CA		EQU 32H

		ORG 1000H
MSJP		DB "PRENDIDA"
MSJA		DB "APAGADA"
FIN		DB ?

		ORG 2000H
		MOV AL, 0FFH
		OUT CA, AL
		IN AL, PA
		AND AL, 80H
		JNZ P
		MOV AL, OFFSET FIN - OFFSET MSJA
		MOV BX, OFFSET MSJA
		INT 7
		JMP FIN
P:		MOV AL, OFFSET MSJA - OFFSET MSJP
		MOV BX, OFFSET MSJP
		INT 7
FIN:		INT 0
END
